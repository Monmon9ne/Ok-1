{
  "version": 2,
  "builds": [
    { "src": "api/upload.js", "use": "@vercel/node" },
    { "src": "api/raw.js", "use": "@vercel/node" },
    { "src": "index.html", "use": "@vercel/static" }
  ],
  "routes": [
    {
      "src": "/api/upload",
      "methods": ["POST"],
      "dest": "/api/upload.js"
    },
    {
      "src": "/api/raw/(.*)",
      "methods": ["GET"],
      "dest": "/api/raw.js"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
